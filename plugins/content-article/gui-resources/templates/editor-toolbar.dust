<div class="toolbar aloha-dialog navbar-fixed-top">
	<div class="btn-toolbar">
		<div class="btn-group headings" data-editor-ui-component="formatBlock">
			<span data-format-info="true">
				<button class="btn currentHeading" rel="tooltip" title="_('Text Heading');" data-format-current="true">Change Heading</button>
				<button class="btn dropdown-toggle" data-format-toggle="true" data-toggle="dropdown" rel="tooltip" title="Change Heading">
					<span class="caret"></span>
				</button>
			</span>
    		<ul class="dropdown-menu" data-format-content="true">
				<li><a href="javascript:void(0)" class="action changeHeading" data-format="p">_('Normal Text');</a></li>
				<li><a href="javascript:void(0)" class="action changeHeading" data-format="h1">_('Heading');</a></li>
				<li><a href="javascript:void(0)" class="action changeHeading" data-format="h2">_('Subheading');</a></li>
