			<button data-editor-ui-component="italic" id="aloha-component-italic" 
				class="btn  action emphasis" rel="tooltip" title="_('Italics');"><i class="editoricon-italic"></i></button>
			<button data-editor-ui-component="underline" id="aloha-component-underline" 
				class="btn  action underline" rel="tooltip" title="_('Underline');"><i class="editoricon-underline"></i></button>
			<button data-editor-ui-component="strikethrough" id="aloha-component-strikethrough" 
				class="btn  action underline" rel="tooltip" title="_('Strikethrough');"><i class="editoricon-strikethrough"></i></button>
			<button data-editor-ui-component="subscript" id="aloha-component-subscript" 
				class="btn  action underline" rel="tooltip" title="_('Subscript');"><i class="editoricon-subscript"></i></button>
			<button data-editor-ui-component="superscript" id="aloha-component-superscript" 
				class="btn  action underline" rel="tooltip" title="_('Superscript');"><i class="editoricon-superscript"></i></button>
				
