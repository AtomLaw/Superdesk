    			<i class="editoricon-link"></i>
    		</button>
    		
    		<button 
    			data-editor-ui-component="insertImage" id="aloha-component-insert-image"
    			class="btn  action insertImage-oer" rel="tooltip" title="_('Insert Figure');">
    			<i class="editoricon-picture"></i>
    		</button>
    		
    		<!-- button class="btn action insertMath" rel="tooltip" title="_('Insert Math');">x<sup>2</sup></button>
    		<button class="btn  action insertNote" rel="tooltip" title="_('Insert Note');">!</button -->
