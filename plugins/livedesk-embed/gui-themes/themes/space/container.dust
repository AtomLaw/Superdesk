<div class="live-blog">	{+header}		<a id="pintotop" href="#">_('to top');</a>		<p style="display: block;" id="liveblog-status" class="update-time">_('updated on ');<span class="new-notification">_('new posts');</span></p>			{/header}	{+list}		<div id="liveblog-posts">			<ol id="liveblog-post-list" class="liveblog-post-list">				<li class="loading-more" id="load-more" style="display:none">					<a name="none"></a>					<a class="liveblog-more" id="liveblog-more">_('Load top Posts');</a>				</li>							</ol>		</div>	{/list}</div>